{
  "extends": [
    "airbnb-base",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier",
    "eslint-plugin-standard"
  ],
  "rules": {
    "prettier/prettier": [
      "error"
    ]
  },
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module"
  },
  "env": {
    "es6": true,
    "browser": false,
    "node": true,
    "commonjs": true
  }
}
